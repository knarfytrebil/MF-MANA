body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,p,blockquote,th,td{margin:0;padding:0;}
fieldset,img{border:0;}address,caption,cite,code,dfn,em,th,var{font-style:normal;font-weight:normal;}
ol,ul{list-style:none;}caption,th {text-align:left;}h1,h2,h3,h4,h5,h6{font-size:100%;}q:before,q:after{content:'';}
table{table-layout:fixed;}

i{overflow:hidden;display:inline-block;}
iframe{background-color:white;}
input[type="radio"],input[type="checkbox"]{margin-right:2px;#margin-right:0;}
input{font-size:12px;font-family:Tahoma,Arial, Helvetica, sans-serif;-moz-background-clip:border;-moz-background-inline-policy:continuous;-moz-background-origin:padding;outline-color:-moz-use-text-color;outline-style:none;outline-width:medium;}
label input{position:relative;top:2px;#top:1px;font-size:12px;}
textarea{overflow:auto;padding:1px 0 0 1px;font-size:12px;}
select{font-size:12px;}
cite{line-height:33px;display:none;text-indent:-999px;}
table { caption-side: top; width: auto; border-collapse: collapse; empty-cells: hide; }
body{
	font:normal 14px/16px "microsoft yahei",arial;
	background:#f0f3e0;
}
a{ text-decoration:none; color: #333;}
a:hover{ color: #d0461a;}
li{ list-style:none;}
.clear{ clear:both;}
.clearfix:after {
	visibility: hidden;
	display: block;
	font-size: 0;
	content: " ";
	clear: both;
	height: 0;
}
a img{ border:none;}
.clearfix { display: inline-table; }
/* Hides from IE-mac \*/
* html .clearfix { height: 1%; }
.clearfix { display: block; }
/* End hide from IE-mac */

#wrap{ width:1024px; margin:0 auto;}
#hd{ text-align:center; font-size:24px; font-weight:bold; height:40px; line-height:40px; background:#403732; color: #fff; position:relative;}
#login{ position:absolute; right:10px; top:5px; font-size:14px; font-weight:normal;}
#login a{ color:#fff; padding:0 10px;}
#left-side{ width:266px; float:left; border:1px solid #403732; border-top:none; min-height:650px; background-color: #f0f3e0;}
#left-side ul{ padding:60px 0 0 40px;}
#left-side ul li{ height:42px; line-height:38px; background:url(/static/themes/desktop/user/images/nav_bg.png) no-repeat; margin-bottom:35px; font-size:22px; text-align:center; width:154px;}
#left-side ul li a{ color:#fff;}

#right-side{ width:740px; float:right; padding-left:8px;}
ul.upload{ padding:20px 5px;}
ul.upload li{ width:170px; height:253px; border-radius:3px; border:1px solid #666; float:left; margin-right: 10px; text-align:center; margin-bottom: 15px;}
.zip{ width:104px; height:133px; margin: 15px 30px 25px 33px; background:url(/static/themes/desktop/user/images/zip.png) no-repeat;}
.post-tit{ height:30px; line-height:30px; font-size:13px;}
.post-time{ font-size:12px;font-family:Tahoma;}
.tongji{ padding:25px;}
.fl{ float: left;}
.fr{ float: right;}
.sort{ height:30px; line-height:30px; background-color: #403732; color: #fff; padding:0 15px; margin-top: 20px;}
.sort a{ padding:0 4px; color: #d39b3a;}
.bill{ padding:30px;}
.top{ padding:15px 0 15px 0;}
.done{ color:#999;}
.undone{ color:#333;}
tbody .trlist{ border:1px solid #000; }
.trlist td{ padding-left:10px;}
.question{ padding:20px 0;}
.tdlist{ border:1px solid #000; border-top:none;}
.btn{ text-align:right; font-size:18px;}
.btn a{ padding:0 10px;}